RequiresVersion( 16 );

// Define structures used in BYS files

typedef struct {
    UBYTE   bfStartYear;
    UBYTE   bfStartMonth;
    UBYTE   bfStartDay;
    UBYTE   bfStartHour;
    UBYTE   bfStartMinute;
    UBYTE   bfStartSecond;
} BYSDATETIME;

typedef struct {
    UBYTE       bfMagic[4] <style=sHeading1>; // AAAA (Footer ends with BBBB)
    UBYTE       bfMode; // 0x00=CPAP, 0x01=APAP
    UBYTE       bfRamp; // Minutes
    UBYTE       bfInitialPressure; // Decimal / 10
    UBYTE       bfPressureSetting; // Decimal / 10
    UBYTE       bfMaximumPressure; // Decimal / 10
    UBYTE       bfMinimumPressure; // Decimal / 10
    UBYTE       bfHumidity;
    UBYTE       bfFPSLevel;
    UBYTE       bfUnknown1[19] <style=sHeading4>;
    WORD        bfRecordCount <style=sSection3>;  // Total records
    UBYTE       bfUnknown3[99] <style=sHeading4>; // 0xFF
    CHAR        bfSerial[16] <style=sHeading2Accent>;
    UBYTE       bfUnknown4[144] <style=sHeading4>; // 0xFF
    UBYTE       bfUnknown5[12] <style=sHeading4>;
    UBYTE       bfUnknown7[2768] <style=sHeading4>;
} BYSFILEHEADER <style=sHeading1>;

typedef struct {
    BYSDATETIME bfStart;
    BYSDATETIME bfStop;
    UBYTE    bfMode; // 0x00=CPAP, 0x01=APAP
    UBYTE    bfRamp; // Ramp (minutes)
    UBYTE    bfInitialPressure; // Decimal / 10
    UBYTE    bfPressureSetting; // Decimal / 10
    UBYTE    bfMaximumPressure; // Decimal / 10
    UBYTE    bfMinimumPressure; // Decimal / 10
    UBYTE    bfHumidity;
    UBYTE    bfFPSLevel;
    UBYTE    bfOAICount;
    UBYTE    bfHICount;
    UBYTE    bfCAICount; // Possibly?
    UBYTE    bfUnknown7;
    UBYTE    bfAvgPressure;
    UBYTE    bfAvgLeakVolume;
    WORD     bfRecordOffset; // Big endian for **reasons**
    UBYTE    bfUnknown12;
    UBYTE    bfMinutes; // Total number of lines for the session
} BYSSESSIONSUMMARY;

typedef struct {
    UBYTE    bfLeakage;
    UBYTE    bfPressure;
    UBYTE    bfSPO2;
    UBYTE    bfOAI;
    UBYTE    bfHI;
    UBYTE    bfPulse;
    UBYTE    bfCAI;
} BYSSESSIONLINE;
