typedef struct {
    UBYTE       bfUnknown12[5] <style=sHeading4>; // 'BBBB' and 0xFF
} BYSFILEFOOTER <style=sHeading1>;


BYSFILEHEADER header;
struct BYSSESSIONSUMMARY sessions[header.bfRecordCount] <style=sHeading4>; // Session summary data
// Number of session lines = sum(sessions[x].minutes)
UBYTE       bfUnknown9[22906] <style=sHeading4>; // 0xFF
// This line is a combination of header.bfRecordCount sessions each with
// a footer for each session lines
// Session
// +- Lines
// +- Session footer (? bytes)
struct BYSSESSIONLINE sessionlines[5046] <style=sHeading4>; // Session summary data
BYSFILEFOOTER footer;

